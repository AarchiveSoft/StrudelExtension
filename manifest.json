{
    "manifest_version": 3,
    "name": "Strudel Enhanced Editor",
    "version": "1.0",
    "description": "Adds syntax highlighting and autocomplete to Strudel",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [],
    "content_scripts": [
        {
            "matches": [
                "*://strudel.cc/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "codemirror-extension.js",
                "syntax-definitions.js",
                "repl-monitor.js",
                "strudel-tokenizer.js",
                "autocomplete-data.js",
                "autocomplete.js"
            ],
            "matches": [
                "*://strudel.cc/*"
            ]
        }
    ]
}